  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev:local": "concurrently \"pnpm open-home\" \"next dev\"",
    "dev:safe": "concurrently \"pnpm open-home:safe\" \"dotenv -e .env.https next dev --experimental-https --experimental-https-key ./certificates/localhost-key.pem --experimental-https-cert ./certificates/localhost.pem\"",
    "start": "next start",
    "start:local": "concurrently \"pnpm open-home\" \"pnpm start\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "check-info": "next info",
    "deps": "node ncu.js",
    "local:build": "pnpx @cloudflare/next-on-pages",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "open-home": "open-cli http://localhost:3000",
    "open-home:safe": "open-cli https://localhost:3000",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check && pnpm format:write",
    "clean": "rimraf --glob **/node_modules **/dist **/.next pnpm-lock.yaml",
    "prepare": "husky",
    "typecheck": "tsc --noEmit --pretty"
  },
